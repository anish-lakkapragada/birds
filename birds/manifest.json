{
    "name": "Birds",
    "manifest_version": 3,
    "version": "0.0.0",
    "permissions": [
      "storage", 
      "contextMenus"
    ], 
    "action": { "default_popup": "index.html" }, 
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["src/content_script.jsx"],
        "all_frames": true,
        "world": "MAIN"
      } 
    ], 
    "background": {
      "service_worker": "src/background.jsx"
    }, 
    "web_accessible_resources": [
      {
        "resources": ["src/modal.html", "index.html"],
        "matches": ["<all_urls>"]
      }
    ]
}